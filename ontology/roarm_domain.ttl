@prefix : <http://roarm.ai/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# RoArm M3 Domain Ontology
# 프로젝트의 핵심 도메인 개념과 관계를 정의

:Ontology a owl:Ontology ;
  rdfs:label "RoArm M3 Isaac Sim Domain Ontology" ;
  rdfs:comment "로봇, 시뮬레이션 환경, 개발 프로세스의 개념적 구조" ;
  owl:versionInfo "0.1" .

# ============================================
# 1. 핵심 엔티티 클래스
# ============================================

:Entity a owl:Class ;
  rdfs:label "Entity" ;
  rdfs:comment "모든 온톨로지 개체의 최상위 클래스" .

# 1.1 로봇 도메인
:Robot a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "Robot" .

:RoArmM3 a owl:Class ;
  rdfs:subClassOf :Robot ;
  rdfs:label "RoArm M3 Robot" .

:Link a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "Robot Link" .

:Joint a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "Robot Joint" .

:EndEffector a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "End Effector" .

:Sensor a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "Sensor" .

:Camera a owl:Class ;
  rdfs:subClassOf :Sensor ;
  rdfs:label "Camera Sensor" .

# 1.2 시뮬레이션 도메인
:Simulation a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "Simulation Environment" .

:IsaacSim a owl:Class ;
  rdfs:subClassOf :Simulation ;
  rdfs:label "NVIDIA Isaac Sim" .

:USD a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "Universal Scene Description" .

:USDStage a owl:Class ;
  rdfs:subClassOf :USD ;
  rdfs:label "USD Stage" .

:USDPrim a owl:Class ;
  rdfs:subClassOf :USD ;
  rdfs:label "USD Prim" .

:PhysXEngine a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "PhysX Physics Engine" .

:Extension a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "Isaac Sim Extension" .

# 1.3 환경 및 의존성
:Environment a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "Development Environment" .

:PythonEnvironment a owl:Class ;
  rdfs:subClassOf :Environment ;
  rdfs:label "Python Virtual Environment" .

:SystemComponent a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "System Component" .

:Driver a owl:Class ;
  rdfs:subClassOf :SystemComponent ;
  rdfs:label "Device Driver" .

:Library a owl:Class ;
  rdfs:subClassOf :SystemComponent ;
  rdfs:label "Software Library" .

:PythonModule a owl:Class ;
  rdfs:subClassOf :Library ;
  rdfs:label "Python Module" .

# 1.4 개발 프로세스
:DevelopmentPhase a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "Development Phase" .

:PreflightCheck a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "Preflight Check" .

:SystemCheck a owl:Class ;
  rdfs:subClassOf :PreflightCheck ;
  rdfs:label "System Check" .

:ExtensionCheck a owl:Class ;
  rdfs:subClassOf :PreflightCheck ;
  rdfs:label "Extension Check" .

:USDIntegrityCheck a owl:Class ;
  rdfs:subClassOf :PreflightCheck ;
  rdfs:label "USD Integrity Check" .

# ============================================
# 2. 문제 및 솔루션 클래스
# ============================================

:Problem a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "Problem" ;
  rdfs:comment "프로젝트에서 발생하는 문제" .

:EnvironmentProblem a owl:Class ;
  rdfs:subClassOf :Problem ;
  rdfs:label "Environment Problem" .

:APIProblem a owl:Class ;
  rdfs:subClassOf :Problem ;
  rdfs:label "API Problem" .

:PhysicsProblem a owl:Class ;
  rdfs:subClassOf :Problem ;
  rdfs:label "Physics Problem" .

:LearningProblem a owl:Class ;
  rdfs:subClassOf :Problem ;
  rdfs:label "Learning Problem" .

:Solution a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "Solution" ;
  rdfs:comment "문제에 대한 해결책" .

:ScriptSolution a owl:Class ;
  rdfs:subClassOf :Solution ;
  rdfs:label "Script Solution" .

:DocumentationSolution a owl:Class ;
  rdfs:subClassOf :Solution ;
  rdfs:label "Documentation Solution" .

:ConfigurationSolution a owl:Class ;
  rdfs:subClassOf :Solution ;
  rdfs:label "Configuration Solution" .

# ============================================
# 3. 리소스 클래스
# ============================================

:Resource a owl:Class ;
  rdfs:subClassOf :Entity ;
  rdfs:label "Resource" .

:Document a owl:Class ;
  rdfs:subClassOf :Resource ;
  rdfs:label "Document" .

:Script a owl:Class ;
  rdfs:subClassOf :Resource ;
  rdfs:label "Script" .

:Configuration a owl:Class ;
  rdfs:subClassOf :Resource ;
  rdfs:label "Configuration" .

:ExternalResource a owl:Class ;
  rdfs:subClassOf :Resource ;
  rdfs:label "External Resource" .

# ============================================
# 4. Object Properties (관계)
# ============================================

# 4.1 구조적 관계
:hasPart a owl:ObjectProperty ;
  rdfs:domain :Entity ;
  rdfs:range :Entity ;
  rdfs:label "has part" .

:partOf a owl:ObjectProperty ;
  owl:inverseOf :hasPart ;
  rdfs:label "part of" .

:hasSensor a owl:ObjectProperty ;
  rdfs:domain :Robot ;
  rdfs:range :Sensor ;
  rdfs:label "has sensor" .

:hasEndEffector a owl:ObjectProperty ;
  rdfs:domain :Robot ;
  rdfs:range :EndEffector ;
  rdfs:label "has end effector" .

# 4.2 의존성 관계
:dependsOn a owl:ObjectProperty ;
  rdfs:domain :Entity ;
  rdfs:range :Entity ;
  rdfs:label "depends on" .

:requires a owl:ObjectProperty ;
  rdfs:domain :Entity ;
  rdfs:range :Entity ;
  rdfs:label "requires" .

:conflictsWith a owl:ObjectProperty ;
  rdfs:domain :Entity ;
  rdfs:range :Entity ;
  rdfs:label "conflicts with" ;
  a owl:SymmetricProperty .

# 4.3 문제-솔루션 관계
:hasProblem a owl:ObjectProperty ;
  rdfs:domain :Entity ;
  rdfs:range :Problem ;
  rdfs:label "has problem" .

:hasSolution a owl:ObjectProperty ;
  rdfs:domain :Problem ;
  rdfs:range :Solution ;
  rdfs:label "has solution" .

:solves a owl:ObjectProperty ;
  owl:inverseOf :hasSolution ;
  rdfs:label "solves" .

:causedBy a owl:ObjectProperty ;
  rdfs:domain :Problem ;
  rdfs:range :Entity ;
  rdfs:label "caused by" .

:relatedTo a owl:ObjectProperty ;
  rdfs:domain :Entity ;
  rdfs:range :Entity ;
  rdfs:label "related to" ;
  a owl:SymmetricProperty .

# 4.4 검증 관계
:validates a owl:ObjectProperty ;
  rdfs:domain :Solution ;
  rdfs:range :PreflightCheck ;
  rdfs:label "validates" .

:implements a owl:ObjectProperty ;
  rdfs:domain :Script ;
  rdfs:range :Solution ;
  rdfs:label "implements" .

:documents a owl:ObjectProperty ;
  rdfs:domain :Document ;
  rdfs:range :Entity ;
  rdfs:label "documents" .

# ============================================
# 5. Data Properties (속성)
# ============================================

# 5.1 식별자 및 메타데이터
:id a owl:DatatypeProperty ;
  rdfs:domain :Entity ;
  rdfs:range xsd:string ;
  rdfs:label "identifier" .

:name a owl:DatatypeProperty ;
  rdfs:domain :Entity ;
  rdfs:range xsd:string ;
  rdfs:label "name" .

:description a owl:DatatypeProperty ;
  rdfs:domain :Entity ;
  rdfs:range xsd:string ;
  rdfs:label "description" .

:version a owl:DatatypeProperty ;
  rdfs:domain :Entity ;
  rdfs:range xsd:string ;
  rdfs:label "version" .

# 5.2 상태 및 우선순위
:status a owl:DatatypeProperty ;
  rdfs:domain :Entity ;
  rdfs:range xsd:string ;
  rdfs:label "status" ;
  rdfs:comment "OPEN, IN_PROGRESS, SOLVED, RECURRING, PASS, FAIL" .

:severity a owl:DatatypeProperty ;
  rdfs:domain :Problem ;
  rdfs:range xsd:string ;
  rdfs:label "severity" ;
  rdfs:comment "CRITICAL, HIGH, MEDIUM, LOW" .

:priority a owl:DatatypeProperty ;
  rdfs:domain :Problem ;
  rdfs:range xsd:integer ;
  rdfs:label "priority" ;
  rdfs:comment "1 (highest) to 5 (lowest)" .

# 5.3 시간 정보
:createdAt a owl:DatatypeProperty ;
  rdfs:domain :Entity ;
  rdfs:range xsd:dateTime ;
  rdfs:label "created at" .

:updatedAt a owl:DatatypeProperty ;
  rdfs:domain :Entity ;
  rdfs:range xsd:dateTime ;
  rdfs:label "updated at" .

:solvedAt a owl:DatatypeProperty ;
  rdfs:domain :Problem ;
  rdfs:range xsd:dateTime ;
  rdfs:label "solved at" .

# 5.4 경로 및 위치
:filePath a owl:DatatypeProperty ;
  rdfs:domain :Resource ;
  rdfs:range xsd:string ;
  rdfs:label "file path" .

:url a owl:DatatypeProperty ;
  rdfs:domain :ExternalResource ;
  rdfs:range xsd:anyURI ;
  rdfs:label "URL" .

# 5.5 검증 정보
:checkCommand a owl:DatatypeProperty ;
  rdfs:domain :PreflightCheck ;
  rdfs:range xsd:string ;
  rdfs:label "check command" .

:expectedOutput a owl:DatatypeProperty ;
  rdfs:domain :PreflightCheck ;
  rdfs:range xsd:string ;
  rdfs:label "expected output" .

:actualOutput a owl:DatatypeProperty ;
  rdfs:domain :PreflightCheck ;
  rdfs:range xsd:string ;
  rdfs:label "actual output" .

# 5.6 문제 정보
:symptom a owl:DatatypeProperty ;
  rdfs:domain :Problem ;
  rdfs:range xsd:string ;
  rdfs:label "symptom" ;
  rdfs:comment "문제의 증상 (에러 메시지, 동작 설명)" .

:errorMessage a owl:DatatypeProperty ;
  rdfs:domain :Problem ;
  rdfs:range xsd:string ;
  rdfs:label "error message" .

:occurrenceCount a owl:DatatypeProperty ;
  rdfs:domain :Problem ;
  rdfs:range xsd:integer ;
  rdfs:label "occurrence count" ;
  rdfs:comment "문제 발생 횟수" .

# 5.7 솔루션 정보
:successRate a owl:DatatypeProperty ;
  rdfs:domain :Solution ;
  rdfs:range xsd:float ;
  rdfs:label "success rate" ;
  rdfs:comment "0.0 to 1.0" .

:applicationScope a owl:DatatypeProperty ;
  rdfs:domain :Solution ;
  rdfs:range xsd:string ;
  rdfs:label "application scope" ;
  rdfs:comment "LOCAL, GLOBAL, CONDITIONAL" .

# ============================================
# 6. 개체 (Named Individuals)
# ============================================

# 6.1 우선순위 수준
:CRITICAL a :severity ;
  rdfs:label "CRITICAL" .

:HIGH a :severity ;
  rdfs:label "HIGH" .

:MEDIUM a :severity ;
  rdfs:label "MEDIUM" .

:LOW a :severity ;
  rdfs:label "LOW" .

# 6.2 상태
:OPEN a :status ;
  rdfs:label "OPEN" .

:IN_PROGRESS a :status ;
  rdfs:label "IN_PROGRESS" .

:SOLVED a :status ;
  rdfs:label "SOLVED" .

:RECURRING a :status ;
  rdfs:label "RECURRING" .

:PASS a :status ;
  rdfs:label "PASS" .

:FAIL a :status ;
  rdfs:label "FAIL" .

# 6.3 적용 범위
:LOCAL a :applicationScope ;
  rdfs:label "LOCAL" .

:GLOBAL a :applicationScope ;
  rdfs:label "GLOBAL" .

:CONDITIONAL a :applicationScope ;
  rdfs:label "CONDITIONAL" .
